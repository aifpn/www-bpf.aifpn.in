{
    "type": "module",
    "name": "www-bpf.aifpn.in",
    "version": "1.0.0",
    "scripts": {
        "add:css": "node --loader ts-node/esm scripts/add-css.ts",
        "remove:css": "node --loader ts-node/esm scripts/remove-css.ts",
        "dev": "pnpm run add:css && svelte-kit dev",
        "build:scss": "sass --no-source-map src/styles/global.scss static/global.css",
        "build:pcss": "cross-env TAILWIND_MODE=build NODE_ENV=production cross-env-shell postcss static/global.css -o static/global.css",
        "build:pcss:tw": "cross-env TAILWIND_MODE=build NODE_ENV=production STYLE_TYPE=TAILWIND postcss src/styles/tailwind.postcss -o static/tailwind.css",
        "build:sk-app": "cross-env-shell svelte-kit build",
        "build": "run-s util:clean remove:css build:scss build:pcss build:pcss:tw build:sk-app",
        "build:fresh": "run-s util:clean build",
        "deploy:gh": "node --loader ts-node/esm scripts/deploy.ts",
        "deploy": "run-s build:fresh deploy:gh",
        "publish": "pnpm run deploy",
        "util:clean": "rimraf .svelte-kit && rimraf build",
        "util:check": "svelte-check --tsconfig ./tsconfig.json",
        "util:checkw": "svelte-check --tsconfig ./tsconfig.json --watch",
        "util:format": "prettier --ignore-path .gitignore --write ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js}",
        "util:lint": "prettier --ignore-path .gitignore --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .",
        "util:lintf": "prettier --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .",
        "util:eslint": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .",
        "util:eslintf": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore . --fix"
    },
    "devDependencies": {
        "@squoosh/lib": "^0.4.0",
        "@sveltejs/adapter-static": "next",
        "@sveltejs/kit": "next",
        "@types/node": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "autoprefixer": "^10.4.0",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.14",
        "cssnano-preset-advanced": "^5.1.9",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^3.2.1",
        "gh-pages": "^3.2.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.5",
        "postcss-cli": "^9.1.0",
        "postcss-import": "^14.0.2",
        "postcss-load-config": "^3.1.0",
        "postcss-nested": "^5.0.6",
        "postcss-scss": "^4.0.2",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.5.1",
        "rimraf": "^3.0.2",
        "sass": "^1.45.1",
        "sass-loader": "^12.4.0",
        "svelte": "^3.44.0",
        "svelte-check": "^2.2.6",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.10.1",
        "tailwindcss": "^3.0.7",
        "ts-node": "^10.4.0",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3",
        "vite-imagetools": "^3.6.8",
        "webpack": "^5.65.0"
    }
}
